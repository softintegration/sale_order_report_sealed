#. module: sale_order_report_sealed
#: model:ir.actions.report,print_report_name:sale_order_report_sealed.action_report_saleorder_with_seal
msgid ""
"(object.state in ('draft', 'sent') and 'Sealed Quotation - %s' % "
"(object.name)) or 'Sealed Order - %s' % (object.name)"
msgstr ""
"(object.state in ('draft', 'sent') and 'Devis signé et cacheté- %s' % "
